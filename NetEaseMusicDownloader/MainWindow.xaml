        <Label Content="等待中..." Name="Label_Info" Margin="104,40,0,0" VerticalAlignment="Top" Height="38" HorizontalAlignment="Left" Width="322"/>
        <Image Name="AlbumFrontCovert" HorizontalAlignment="Left" Height="73" Margin="16,42,0,0" VerticalAlignment="Top" Width="73" Source="/07.jpg"/>
