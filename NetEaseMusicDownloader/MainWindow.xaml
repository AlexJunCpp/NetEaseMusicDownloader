        <Label Content="歌曲地址" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="25" Width="134"/>
        <TextBox Margin="69,14,107,0" IsReadOnly="True" Text="" Name="TextBox_Url" TextWrapping="Wrap" VerticalAlignment="Top" Height="17"/>
